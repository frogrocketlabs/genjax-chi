   "source": [
    "import jax.random as jrand\n",
    "\n",
    "import genjax\n",
    "from genjax import gen, pjax_to_jax\n",
    "\n",
    "\n",
    "@gen\n",
    "def model(idx, mu):\n",
    "    return genjax.normal.switch(genjax.normal)(idx, (mu, 2.0), (mu, 1.0)) @ \"x\"\n",
    "\n",
    "\n",
    "pjax_to_jax(jrand.key(5), model.simulate)((0, 0.3))"
   ]
  }
 ],
