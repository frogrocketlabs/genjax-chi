   "source": [
    "import jax.numpy as jnp\n",
    "import jax.random as jrand\n",
    "\n",
    "import genjax\n",
    "from genjax import gen, pjax_to_jax\n",
    "\n",
    "\n",
    "@gen\n",
    "def model(carry, xs):\n",
    "    x = genjax.normal(carry, 1.0) @ \"x\"\n",
    "    return x, xs\n",
    "\n",
    "\n",
    "pjax_to_jax(jrand.key(1), model.scan(n=10).simulate)((0.1, jnp.ones(10)))"
   ]
  },
